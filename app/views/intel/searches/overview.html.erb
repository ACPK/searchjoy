<h4><%= @time_range.first.strftime("%b %-e, %Y") %> to <%= @time_range.last.strftime("%b %-e, %Y") %></h4>

<h1>Searches</h1>

<%= line_chart @searches_by_week %>

<h1>Top Searches</h1>
<%= link_to "View all", intel.searches_path(search_type: params[:search_type]) %>
<% @top_searches.each do |row| %>
  <p><%= row %></p>
<% end %>

<h1>Low Conversions</h1>
<%= link_to "View all", intel.searches_path(search_type: params[:search_type], sort: "conversion_rate") %>
<% @bad_conversion_rate.each do |row| %>
  <p><%= row %></p>
<% end %>
